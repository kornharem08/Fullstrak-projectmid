<% include ../partials/header %>

<table class = "table">
    <thead>
        <tr>
        <td>ID</td>
        <td>email</td>
        <td>password</td>
        </tr>
</thead>
    <tbody>
    <% users.forEach(function(user){ %>
        <tr>
            <td scope ="row"><%= user.id %></td>
            <td><%= user.email %></td>
            <td><%= user.password %>></td>
    </tr>
    <% }); %>
    </tbody>
    </table>

